    "@types/react-dom": "^19.2.2",
    "husky": "^9.1.7",
    "react-router-devtools": "^5.1.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
